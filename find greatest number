#include <stdio.h>
int max(int a, int b,int c); //function prototype
int max(int a,int b, int c){
      if(a>b && a>c){
          return a;
      }
      else if(b>a && b>c){
          return b;
      }
      else{
          return c;
      }
}
int main() {
    int x,y,z;
    scanf("%d %d %d", &x, &y, &z);
    int result = max(x,y,z);
     printf("greatest %d",result);
   

    return 0;
}
