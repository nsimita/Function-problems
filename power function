#include <stdio.h>
int power();
int main() {
   int result,x,y;
  printf("enter base: ");
  scanf("%d",&x);
  printf("enter power: ");
  scanf("%d",&y);
   result=power(x,y);
   printf("%d",result);

    return 0;
}
int power(int x, int y){
    int p=1;
     for(int i=1; i<=y; i++){
         p=p*x;
     }
     
    return p;
}
